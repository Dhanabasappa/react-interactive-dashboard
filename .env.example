# OpenWeather API

VITE_OPENWEATHER_API_KEY=your_openai_api_key_here

# CoinGecko API
VITE_COINGECKO_API_URL=https://api.coingecko.com/api/v3

# Application Settings
VITE_APP_NAME=Interactive Dashboard
VITE_MAX_FILE_SIZE_MB=5
